{
  "Version": 1,
  "TypeFullName": "CommandSystem.Commands.User.GetValue",
  "Description": "Gets the value of a field or property.",
  "Aliases": ["getvalue", "gv"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to get the value from.",
          "Required": true
        },
        {
          "Type": "System.String",
          "Name": "{Field or Property Name}",
          "Description": "The name of the field or property to get the value of.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "CSharp": "CommandSystem.Commands.Utility.GetValue({Object}, {Field or Property Name})"
        }
      ],
      "Output": {
        "Type": "System.Object",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Got Value: {Value}"
    }
  ]
}