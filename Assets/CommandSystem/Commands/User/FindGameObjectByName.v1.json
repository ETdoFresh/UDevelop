{
  "Version": 1,
  "TypeFullName": "CommandSystem.Commands.User.FindGameObjectByName",
  "Description": "Finds a GameObject by name",
  "Aliases": ["findgameobjectbyname", "findgo", "fg"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{GameObject Name}",
          "Description": "The name of the GameObject to find.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.Object[]",
          "Name": "{All GameObjects}",
          "Command": "FindGameObjectsByName {GameObject Name}"
        },
        {
          "Type": "UnityEngine.Object",
          "Name": "{GameObject}",
          "CSharp": "System.Linq.Enumerable.FirstOrDefault({All GameObjects})"
        }
      ],
      "Output": {
        "Type": "UnityEngine.Object",
        "Name": "{GameObject}"
      },
      "CommandLineOutput": "Found GameObject: {GameObject}"
    }
  ]
}