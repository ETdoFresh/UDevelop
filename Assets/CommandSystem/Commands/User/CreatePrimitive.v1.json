{
  "Version": 1,
  "TypeFullName": "CommandSystem.Commands.User.CreatePrimitive",
  "Description": "Creates a primitive object.",
  "Aliases": ["createprimitive", "createprim"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "UnityEngine.PrimitiveType",
          "Name": "{Primitive Type}",
          "Description": "The type of primitive to create.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{Primitive GameObject}",
          "CSharp": "UnityEngine.GameObject.CreatePrimitive({Primitive Type})"
        }
      ],
      "Output": {
        "Type": "UnityEngine.GameObject",
        "Name": "{Primitive GameObject}"
      },
      "CommandLineOutput": "Created primitive: {Primitive Type}"
    }
  ]
}