{
  "Version": 1,
  "Name": "SetValueByGameObjectName",
  "Description": "Sets the value of a field or property.",
  "Aliases": ["setvaluebygoname", "svbg"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.String",
          "Name": "{GameObject Name}",
          "Description": "The name of the GameObject to set the value of.",
          "Required": true
        },
        {
          "Type": "System.String",
          "Name": "{Field or Property Name}",
          "Description": "The name of the field or property to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "Description": "The value to set the field or property to.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Command": "FindGameObjectByName {GameObject Name}"
        },
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "CommandSystem.Commands.Utility.SetValue({GameObject}, {Field or Property Name}, {Value})"
        }
      ],
      "Output": {
        "Type": "System.Void",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Set Value: {Field or Property Name} = {Value}"
    }
  ]
}