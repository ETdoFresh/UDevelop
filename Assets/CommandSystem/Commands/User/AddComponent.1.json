{
  "Version": 1,
  "TypeFullName": "CommandSystem.Commands.User.AddComponent",
  "Description": "Adds a component to a GameObject.",
  "Aliases": ["addcomponent", "addcomp"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Description": "The GameObject to add the component to.",
          "Required": true
        },
        {
          "Type": "System.Type",
          "Name": "{Component Type}",
          "Description": "The type of component to add.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.Component",
          "Name": "{Component}",
          "CSharp": "UnityEngine.GameObject.AddComponent(this {GameObject}, {Component Type})"
        }
      ],
      "Output": {
        "Type": "UnityEngine.Component",
        "Name": "{Component}"
      },
      "CommandLineOutput": "Added component: {Component Type}"
    },
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{GameObject Name}",
          "Description": "The GameObject to add the component to.",
          "Required": true
        },
        {
          "Type": "string",
          "Name": "{Component Type Name}",
          "Description": "The type of component to add.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Command": "FindGameObjectByName {GameObject Name}"
        },
        {
          "Type": "System.Type",
          "Name": "{Component Type}",
          "Command": "FindSystemTypeByName {Component Type Name}"
        },
        {
          "Type": "UnityEngine.Component",
          "Name": "{Component}",
          "CSharp": "UnityEngine.GameObject.AddComponent(this {GameObject}, {Component Type})"
        }
      ],
      "Output": {
        "Type": "UnityEngine.Component",
        "Name": "{Component}"
      },
      "CommandLineOutput": "Added component: {Component Type}"
    }
  ]
}