{
  "Version": 1,
  "Name": "GetComponent",
  "Description": "Gets a component from a GameObject.",
  "Aliases": ["getcomponent", "getcomp"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Description": "The GameObject to get the component from.",
          "Required": true
        },
        {
          "Type": "System.Type",
          "Name": "{Component Type}",
          "Description": "The type of component to get.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.Component",
          "Name": "{Component}",
          "CSharp": "UnityEngine.GameObject.GetComponent(this {GameObject}, {Component Type})"
        }
      ],
      "Output": {
        "Type": "UnityEngine.Component",
        "Name": "{Component}"
      },
      "CommandLineOutput": "Got component: {Component Type}"
    },
    {
