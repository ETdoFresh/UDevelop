{
  "Version": 1,
  "TypeFullName": "CommandSystem.Commands.User.FindPrefabByNameAndCreateInstanceCommand",
  "Description": "Finds a prefab by name and creates an instance of it.",
  "Aliases": ["findprefabbynameandcreateinstance", "findprefabandcreateinstance", "findandcreateinstance", "fpaci"],
  "Group": "Create",
  "Arguments": [
    {
      "Name": "Prefab Name",
      "Description": "The name of the prefab to find and create an instance of.",
      "Type": "string",
      "Required": true,
      "Default": ""
    },
    {
      "Type": "string",
      "Name": "Instance Name",
      "Description": "The name of the instance to create.",
      "Required": false,
      "Default": ""
    },
    {
      "Type": "string",
      "Name": "Parent Name",
      "Description": "The name of the parent to create the instance under.",
      "Required": false,
      "Default": ""
    },
    {
      "Type": "UnityEngine.Vector3",
      "Name": "Position",
      "Description": "The position of the instance to create.",
      "Required": false,
      "Default": ""
    },
    {
      "Type": "UnityEngine.Quaternion",
      "Name": "Rotation",
      "Description": "The rotation of the instance to create.",
      "Required": false,
      "Default": ""
    }
  ],
  "Steps": [
    {
      "StepType": "FunctionCall",
      "TypeFullName": "CommandSystem.FunctionCalls.FindPrefabByName",
      "Function": "FindPrefabByName",
      "Arguments": [["{Prefab Name}"]],
      "Output": {
        "Type": "UnityEngine.GameObject",
        "Name": "{Found Prefab}"
      }
    },
    {
      "StepType": "FunctionCall",
      "TypeFullName": "CommandSystem.FunctionCalls.FindGameObjectByName",
      "Function": "FindGameObjectByName",
      "Arguments": [["{Parent Name}"]],
      "Output": {
        "Type": "UnityEngine.GameObject",
        "Name": "{Found Parent}"
      }
    },
    {
      "StepType": "FunctionCall",
      "TypeFullName": "UnityEngine.Object",
      "Function": "Instantiate",
      "Arguments": [
        ["{Found Prefab}", "{Position}", "{Rotation}", "{Found Parent}"],
        ["{Found Prefab}", "{Position}", "{Rotation}"],
        ["{Found Prefab}", "{Found Parent}"],
        ["{Found Prefab}"]
      ],
      "Output": {
        "Type": "UnityEngine.GameObject",
        "Name": "{Created Instance}"
      }
    },
    {
      "StepType": "FunctionCall",
      "TypeFullName": "CommandSystem.FunctionCalls.GetPrefabName",
      "Function": "GetPrefabName",
      "Arguments": [["{Found Prefab}, {Instance Name}"]],
      "Output": {
        "Type": "string",
        "Name": "{Prefab or Instance Name}"
      }
    },
    {
      "StepType": "PropertySet",
      "TypeFullName": "UnityEngine.GameObject",
      "Property": "name",
      "Value": "{Prefab or Instance Name}"
    },
    {
      "TypeFullName": "CommandSystem.FunctionCalls.SelectGameObject",
      "Function": "SelectGameObject",
      "Arguments": [["{Created Instance}"]]
    }
  ]
}