{
  "Version": 1,
  "TypeFullName": "CommandSystem.Commands.User.SetValue",
  "Description": "Sets the value of a field or property.",
  "Aliases": ["setvalue", "sv"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to set the value of.",
          "Required": true
        },
        {
          "Type": "System.String",
          "Name": "{Field or Property Name}",
          "Description": "The name of the field or property to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "Description": "The value to set the field or property to.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Void",
          "Name": "{Value}",
          "CSharp": "CommandSystem.Commands.Utility.SetValue({Object}, {Field or Property Name}, {Value})"
        }
      ],
      "Output": {
        "Type": "System.Void",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Set Value: {Field or Property Name} = {Value}"
    }
  ]
}