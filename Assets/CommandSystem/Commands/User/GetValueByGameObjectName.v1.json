{
  "Version": 1,
  "Name": "GetValueByGameObjectName",
  "Description": "Gets the value of a field or property.",
  "Aliases": ["getvaluebygoname", "gvbg"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.String",
          "Name": "{GameObject Name}",
          "Description": "The name of the GameObject to get the value from.",
          "Required": true
        },
        {
          "Type": "System.String",
          "Name": "{Field or Property Name}",
          "Description": "The name of the field or property to get the value of.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Command": "FindGameObjectByName {GameObject Name}"
        },
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "Command": "GetValue {GameObject} {Field or Property Name}"
        }
      ],
      "Output": {
        "Type": "System.Object",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Got Value: {Value}"
    }
  ]
}