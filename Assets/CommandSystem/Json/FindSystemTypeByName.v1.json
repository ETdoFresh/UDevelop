{
  "Version": 1,
  "Name": "FindSystemTypeByName",
  "Description": "Finds a System.Type by name",
  "Author": "ETdoFresh",
  "Aliases": ["findsystemtypebyname", "findtype", "ft"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{Type Name}",
          "Description": "The name of the System.Type to find.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Type",
          "Name": "{Type}",
          "CSharp": "CommandSystem.Commands.Utility.FindSystemTypeByName({Type Name})"
        }
      ],
      "Output": {
        "Type": "System.Type",
        "Name": "{Type}"
      },
      "CommandLineOutput": "Found Type: {Type}"
    }
  ]
}