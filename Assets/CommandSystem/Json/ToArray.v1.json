{
  "Name": "ToArray",
  "Version": 1,
  "Description": "Converts an object to an array of one object",
  "Author": "ETdoFresh",
  "Aliases": ["toarray"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "object",
          "Name": "{Value}",
          "Description": "The value to convert",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "object[]",
          "Name": "{Value}",
          "CSharp": "CommandSystem.Commands.Utility.ToArray({Value})"
        }
      ],
      "Output": {
        "Type": "object[]",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Value: {Value}"
    }
  ]
}