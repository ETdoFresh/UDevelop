{
  "Name": "GetValue",
  "Version": 1,
  "Description": "Gets the value of a field or property.",
  "Author": "ETdoFresh",
  "Aliases": ["getvalue", "gv"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.String",
          "Name": "{Field or Property Name}",
          "Description": "The name of the field or property to get the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to get the value from.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Type",
          "Name": "{Object Type}",
          "Command": "GetType {Object}"
        },
        {
          "Type": "System.Object",
          "Name": "{Field or Property}",
          "Command": "GetFieldInfoOrPropertyInfo {Object Type} {Field or Property Name}"
        },
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "Command": "GetValue {Field or Property} {Object}"
        }
      ],
      "Output": {
        "Type": "System.Object",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Got Value: {Value}"
    },
    {
      "Input": [
        {
          "Type": "System.Reflection.FieldInfo",
          "Name": "{Field}",
          "Description": "The field to get the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to get the value from.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "CSharp": "System.Reflection.FieldInfo.GetValue(this {Field}, {Object})"
        }
      ],
      "Output": {
        "Type": "System.Object",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Got Value: {Value}"
    },
    {
      "Input": [
        {
          "Type": "System.Reflection.PropertyInfo",
          "Name": "{Property}",
          "Description": "The property to get the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to get the value from.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "CSharp": "System.Reflection.PropertyInfo.GetValue(this {Property}, {Object})"
        }
      ],
      "Output": {
        "Type": "System.Object",
        "Name": "{Value}"
      },
      "CommandLineOutput": "Got Value: {Value}"
    }
  ]
}