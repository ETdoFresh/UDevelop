{
  "Version": 1,
  "Name": "GetPropertyInfo",
  "Description": "Gets the PropertyInfo from type and property name",
  "Aliases": ["getpropertyinfo", "gpi"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.Type",
          "Name": "{Type}",
          "Description": "The type to get the property from.",
          "Required": true
        },
        {
          "Type": "string",
          "Name": "{Property Name}",
          "Description": "The name of the property to get.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Reflection.BindingFlags",
          "Name": "{Binding Flags}",
          "CSharp": "Instance | Static | Public | NonPublic"
        },
        {
          "Type": "System.Reflection.PropertyInfo",
          "Name": "{Property}",
          "CSharp": "System.Type.GetProperty(this {Type}, {Property Name}, {Binding Flags})"
        }
      ],
      "Output": {
        "Type": "System.Reflection.PropertyInfo",
        "Name": "{Property}"
      },
      "CommandLineOutput": "Found Property: {Property}"
    },
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{Type Name}",
          "Description": "The name of the type to get the property from.",
          "Required": true
        },
        {
          "Type": "string",
          "Name": "{Property Name}",
          "Description": "The name of the property to get.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Type",
          "Name": "{Type}",
          "Command": "FindSystemTypeByName {Type Name}"
        },
        {
          "Type": "System.Reflection.BindingFlags",
          "Name": "{Binding Flags}",
          "CSharp": "Instance | Static | Public | NonPublic"
        },
        {
          "Type": "System.Reflection.PropertyInfo",
          "Name": "{Property}",
          "CSharp": "System.Type.GetProperty(this {Type}, {Property Name}, {Binding Flags})"
        }
      ],
      "Output": {
        "Type": "System.Reflection.PropertyInfo",
        "Name": "{Property}"
      },
      "CommandLineOutput": "Found Property: {Property}"
    }
  ]
}