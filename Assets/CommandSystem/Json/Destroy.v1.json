{
  "Name": "Destroy",
  "Version": 1,
  "Description": "Destroys an Object.",
  "Author": "ETdoFresh",
  "Aliases": ["destroy"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "UnityEngine.Object",
          "Name": "{Object}",
          "Description": "The Object to destroy.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "string",
          "Name": "{Object ScenePath}",
          "Command": "GetScenePath {Object}"
        },
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "UnityEngine.Object.DestroyImmediate({Object})"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Destroyed Object: {Object ScenePath}"
    },
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{GameObject Name}",
          "Description": "The name of the GameObject to destroy.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Command": "FindGameObjectByName {GameObject Name}"
        },
        {
          "Type": "string",
          "Name": "{GameObject ScenePath}",
          "Command": "GetScenePath {GameObject}"
        },
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "UnityEngine.Object.DestroyImmediate({GameObject})"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Destroyed GameObject: {GameObject ScenePath}"
    }
  ]
}