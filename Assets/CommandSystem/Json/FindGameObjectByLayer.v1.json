{
  "Version": 1,
  "Name": "FindGameObjectByLayer",
  "Description": "Find GameObject by layer.",
  "Author": "ETdoFresh",
  "Aliases": ["findgameobjectbylayer", "fgobl"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "object",
          "Name": "{Layer}",
          "Description": "The name of the layer to search for.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.Object[]",
          "Name": "{GameObjects}",
          "Command": "FindGameObjectsByLayer {Layer}"
        },
        {
          "Type": "UnityEngine.Object",
          "Name": "{GameObject}",
          "Command": "GetFirstOrDefault {GameObjects}"
        },
        {
          "Type": "string",
          "Name": "{GameObject ScenePath}",
          "Command": "GetScenePath {GameObject}"
        }
      ],
      "Output": {
        "Type": "UnityEngine.Object",
        "Name": "{GameObject}"
      },
      "CommandLineOutput": "Found GameObject:\n{GameObject ScenePath}"
    }
  ]
}