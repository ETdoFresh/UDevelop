{
  "Version": 1,
  "Name": "FindGameObjectsByName",
  "Description": "Find GameObjects by name",
  "Author": "ETdoFresh",
  "Aliases": ["findgameobjectsbyname", "findgos", "fgs"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{GameObject Name}",
          "Description": "The name of the GameObject to find.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.Object[]",
          "Name": "{All GameObjects}",
          "Command": "FindObjectsOfType UnityEngine.GameObject"
        },
        {
          "Type": "UnityEngine.Object[]",
          "Name": "{GameObjects}",
          "Command": "FilterBy name {GameObject Name} {All GameObjects}"
        },
        {
          "Type": "string",
          "Name": "{GameObjects ScenePath}",
          "Command": "GetScenePath {GameObjects}"
        }
      ],
      "Output": {
        "Type": "UnityEngine.Object[]",
        "Name": "{GameObjects}"
      },
      "CommandLineOutput": "Found GameObject:\n{GameObjects ScenePath}"
    }
  ]
}