{
  "Name": "SetValue",
  "Version": 1,
  "Description": "Sets the value of a field or property.",
  "Author": "ETdoFresh",
  "Aliases": ["setvalue", "sv"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.String",
          "Name": "{Field or Property Name}",
          "Description": "The name of the field or property to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "Description": "The value to set the field or property to.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Type",
          "Name": "{Object Type}",
          "Command": "GetType {Object}"
        },
        {
          "Type": "System.Object",
          "Name": "{Field or Property}",
          "Command": "GetFieldInfoOrPropertyInfo {Object Type} {Field or Property Name}"
        },
        {
          "Type": "void",
          "Name": "void",
          "Command": "SetValue {Field or Property} {Object} {Value}"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Set Value: {Value}"
    },
    {
      "Input": [
        {
          "Type": "System.Reflection.FieldInfo",
          "Name": "{Field}",
          "Description": "The field to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "Description": "The value to set the field to.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "System.Reflection.FieldInfo.SetValue(this {Field}, {Object}, {Value})"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Set Value: {Value}"
    },
    {
      "Input": [
        {
          "Type": "System.Reflection.PropertyInfo",
          "Name": "{Property}",
          "Description": "The property to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Object}",
          "Description": "The object to set the value of.",
          "Required": true
        },
        {
          "Type": "System.Object",
          "Name": "{Value}",
          "Description": "The value to set the property to.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "System.Reflection.PropertyInfo.SetValue(this {Property}, {Object}, {Value})"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Set Value: {Value}"
    }
  ]
}