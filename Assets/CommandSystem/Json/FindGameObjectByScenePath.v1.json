{
  "Version": 1,
  "Name": "FindGameObjectByScenePath",
  "Description": "Finds a GameObject by scene path",
  "Author": "ETdoFresh",
  "Aliases": ["findgameobjectbyscenepath", "findgobsp", "fgobsp"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{Scene Path}",
          "Description": "The scene path of the GameObject to find.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject[]",
          "Name": "{All GameObjects}",
          "Command": "FindObjectsOfType UnityEngine.GameObject"
        },
        {
          "Type": "CommandSystem.CommandReference",
          "Name": "{GetScenePath Command}",
          "Command": "GetCommandReference GetScenePath"
        },
        {
          "Type": "UnityEngine.GameObject[]",
          "Name": "{GameObjects}",
          "Command": "FilterBy {GetScenePath Command} {Scene Path} {All GameObjects}"
        },
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Command": "GetFirstOrDefault {GameObjects}"
        }
      ],
      "Output": {
        "Type": "UnityEngine.Object",
        "Name": "{GameObject}"
      },
      "CommandLineOutput": "Found GameObject: {GameObject}"
    }
  ]
}