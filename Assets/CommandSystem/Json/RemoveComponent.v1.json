{
  "Version": 1,
  "Name": "RemoveComponent",
  "Description": "Removes a component from a GameObject.",
  "Aliases": ["removecomponent", "removecomp"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "UnityEngine.Component",
          "Name": "{Component}",
          "Description": "The component to remove.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "UnityEngine.Object.DestroyImmediate({Component})"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Removed component: {Component}"
    },
    {
      "Input": [
        {
            "Type": "string",
            "Name": "{GameObject Name}",
            "Description": "The name of the GameObject to remove the component from.",
            "Required": true
        },
        {
          "Type": "string",
          "Name": "{Component Name}",
          "Description": "The name of the component to remove.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Command": "FindGameObjectByName {GameObject Name}"
        },
        {
          "Type": "UnityEngine.Component",
          "Name": "{Component}",
          "Command": "GetComponent {GameObject} {Component Name}"
        },
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "UnityEngine.Object.DestroyImmediate({Component})"
        }
      ],
      "Output": {
        "Type": "void",
