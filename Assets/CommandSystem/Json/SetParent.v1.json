{
  "Name": "SetParent",
  "Version": 1,
  "Description": "Sets the parent of a GameObject",
  "Author": "ETdoFresh",
  "Aliases": ["setparent", "sp"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Description": "The GameObject to set the parent of.",
          "Required": true
        },
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{Parent}",
          "Description": "The GameObject to set as the parent.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "string",
          "Name": "{Previous GameObject ScenePath}",
          "Command": "GetScenePath {GameObject}"
        },
        {
          "Type": "UnityEngine.Transform",
          "Name": "{GameObject Transform}",
          "Command": "GetValue transform {GameObject}"
        },
        {
          "Type": "UnityEngine.Transform",
          "Name": "{Parent Transform}",
          "Command": "GetValue transform {Parent}"
        },
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "UnityEngine.Transform.SetParent(this {GameObject Transform}, {Parent Transform})"
        },
        {
          "Type": "string",
          "Name": "{Current GameObject ScenePath}",
          "Command": "GetScenePath {GameObject}"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Moved from {Previous GameObject ScenePath} to {Current GameObject ScenePath}"
    },
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{GameObject Name}",
          "Description": "The GameObject to set the parent of.",
          "Required": true
        },
        {
          "Type": "string",
          "Name": "{Parent ScenePath}",
          "Description": "The GameObject to set as the parent.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{GameObject}",
          "Command": "FindGameObjectByScenePath {GameObject Name}"
        },
        {
          "Type": "string",
          "Name": "{Previous GameObject ScenePath}",
          "Command": "GetScenePath {GameObject}"
        },
        {
          "Type": "UnityEngine.Transform",
          "Name": "{GameObject Transform}",
          "Command": "GetValue transform {GameObject}"
        },
        {
          "Type": "UnityEngine.GameObject",
          "Name": "{Parent}",
          "Command": "FindGameObjectByScenePath {Parent ScenePath}"
        },
        {
          "Type": "UnityEngine.Transform",
          "Name": "{Parent Transform}",
          "Command": "GetValue transform {Parent}"
        },
        {
          "Type": "void",
          "Name": "void",
          "CSharp": "UnityEngine.Transform.SetParent(this {GameObject Transform}, {Parent Transform})"
        },
        {
            "Type": "string",
            "Name": "{Current GameObject ScenePath}",
            "Command": "GetScenePath {GameObject}"
        }
      ],
      "Output": {
        "Type": "void",
        "Name": "void"
      },
      "CommandLineOutput": "Moved from {Previous GameObject ScenePath} to {Current GameObject ScenePath}"
    }
  ]
}