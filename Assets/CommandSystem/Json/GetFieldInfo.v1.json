{
  "Version": 1,
  "Name": "GetFieldInfo",
  "Description": "Gets the FieldInfo from type and field name",
  "Aliases": ["getfieldinfo", "gfi"],
  "Overloads": [
    {
      "Input": [
        {
          "Type": "System.Type",
          "Name": "{Type}",
          "Description": "The type to get the field from.",
          "Required": true
        },
        {
          "Type": "string",
          "Name": "{Field Name}",
          "Description": "The name of the field to get.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Reflection.BindingFlags",
          "Name": "{Binding Flags}",
          "CSharp": "Instance | Static | Public | NonPublic"
        },
        {
          "Type": "System.Reflection.FieldInfo",
          "Name": "{Field}",
          "CSharp": "System.Type.GetField(this {Type}, {Field Name}, {Binding Flags})"
        }
      ],
      "Output": {
        "Type": "System.Reflection.FieldInfo",
        "Name": "{Field}"
      },
      "CommandLineOutput": "Found Field: {Field}"
    },
    {
      "Input": [
        {
          "Type": "string",
          "Name": "{Type Name}",
          "Description": "The name of the type to get the field from.",
          "Required": true
        },
        {
          "Type": "string",
          "Name": "{Field Name}",
          "Description": "The name of the field to get.",
          "Required": true
        }
      ],
      "Calls": [
        {
          "Type": "System.Type",
          "Name": "{Type}",
          "Command": "FindSystemTypeByName {Type Name}"
        },
        {
          "Type": "System.Reflection.BindingFlags",
          "Name": "{Binding Flags}",
          "CSharp": "Instance | Static | Public | NonPublic"
        },
        {
          "Type": "System.Reflection.FieldInfo",
          "Name": "{Field}",
          "CSharp": "System.Type.GetField(this {Type}, {Field Name}, {Binding Flags})"
        }
      ],
      "Output": {
        "Type": "System.Reflection.FieldInfo",
        "Name": "{Field}"
      },
      "CommandLineOutput": "Found Field: {Field}"
    }
  ]
}